<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="CrazyCajun" name="Crazy Cajun Repo" version="1.0" provider-name="123KingKong">
    <extension point="xbmc.addon.repository" name="Crazy Cajun Repo">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/123KingKong/CrazyCajun/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/123KingKong/CrazyCajun/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/123KingKong/CrazyCajun/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Crazy Cajun Repo</summary>
        <description>Offical Crazy Cajun Repo</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

<addon id="plugin.program.Buildwizard" name="Chains Build wizard" version="2.0.3" provider-name="absolution Builds">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
	</extension>
	<extension point="xbmc.service" library="startup.py" start="startup" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Chains Build wizard</summary>
		<description lang="en">Chains Build Wizard allows you to get access to the latest Chains Custom Builds And Themes. Thanks whufclee at NaN, GUYS at TI, Brackets for the app installer!! RomanVM for the PyXBMCt libs. Fire TV Guru for the PyXBMCt layout  Also a very special Thanks to Surfacingx for the base code used in this add-on.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
